<!-- Jumbotron Section -->
<div class="text-center py-5">
  {{#if user}}
    <h1 class="display-4 mb-3 fw-bolder main-txt">{{__ "welcomeUser"}}
      {{user.username}}!</h1>
    <p class="lead mb-4 main-txt2">{{__ "trackAndManage"}}</p>
  {{else}}
    <h1 class="display-4 mb-3 fw-bolder main-txt">{{__ "welcomeTitle"}}</h1>
    <p class="lead mb-4 main-txt2">{{__ "trackAndManage"}}</p>
  {{/if}}
  <hr class="my-4 border-light" />
  {{#unless user}}
    <p class="lead mb-4">{{__ "getStarted"}}</p>
    <a class="btn btn-light btn-lg mr-2" href="/users/login" role="button">{{__
        "login"
      }}</a>
    <a class="btn btn-light btn-lg" href="/users/register" role="button">{{__
        "register"
      }}</a>
  {{/unless}}
</div>

<!-- Features Section -->
<section class="features py-5 border-top border-dark">
  <div class="container">
    <h2 class="text-center mb-4">{{__ "featuresTitle"}}</h2>
    <div class="row">
      <div class="col-md-4 text-center mb-4">
        <div class="feature-icon mb-3">
          <img
            src="https://miro.medium.com/v2/0*PFf6wMuUkN9iI1LU"
            alt="{{__ 'trackApplications'}}"
            class="img-fluid"
          />
        </div>
        <h4>{{__ "trackApplications"}}</h4>
        <p>{{__ "trackApplicationsDesc"}}</p>
      </div>
      <div class="col-md-4 text-center mb-4">
        <div class="feature-icon mb-3">
          <img
            src="https://simpletexting.com/wp-content/uploads/2022/05/text-messages-not-sending-768x432.jpeg"
            alt="{{__ 'shareProfile'}}"
            class="img-fluid"
          />
        </div>
        <h4>{{__ "shareProfile"}}</h4>
        <p>{{__ "shareProfileDesc"}}</p>
      </div>
      <div class="col-md-4 text-center mb-4">
        <div class="feature-icon mb-3">
          <img
            src="data:image/jpeg;base64,"
            alt="{{__ 'downloadReports'}}"
            class="img-fluid"
          />
        </div>
        <h4>{{__ "downloadReports"}}</h4>
        <p>{{__ "downloadReportsDesc"}}</p>
      </div>
    </div>
  </div>
</section>

<!-- How It Works Section -->
<section class="how-it-works py-5 my-5 border-top border-dark">
  <div class="container">
    <h2 class="text-center mb-4">{{__ "howItWorksTitle"}}</h2>
    <div class="row">
      <div class="col-md-4 text-center mb-4">
        <div class="how-it-works-step mb-3">
          <img
            src="https://static.vecteezy.com/system/resources/previews/019/552/601/non_2x/sign-up-icon-signup-square-box-on-transparent-background-free-png.png"
            alt="{{__ 'signUp'}}"
            class="img-fluid"
          />
        </div>
        <h4>{{__ "signUp"}}</h4>
        <p>{{__ "signUpDesc"}}</p>
      </div>
      <div class="col-md-4 text-center mb-4">
        <div class="how-it-works-step mb-3">
          <img
            src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQFPvPk36tgrbVaswnLWLDXOMo_LH4oaJIOMdrJZVoO7uWSdSDzcWB-nasL4Eem-n5ViAo&usqp=CAU"
            alt="{{__ 'addApplications'}}"
            class="img-fluid"
          />
        </div>
        <h4>{{__ "addApplications"}}</h4>
        <p>{{__ "addApplicationsDesc"}}</p>
      </div>
      <div class="col-md-4 text-center mb-4">
        <div class="how-it-works-step mb-3">
          <img
            src="data:image/jpeg;base64,"
            alt="{{__ 'getInsights'}}"
            class="img-fluid"
          />
        </div>
        <h4>{{__ "getInsights"}}</h4>
        <p>{{__ "getInsightsDesc"}}</p>
      </div>
    </div>
  </div>
</section>

<!-- Testimonials Section -->
<section
  class="testimonials py-5 my-5 bg-dark text-light border-top border-light"
>
  <div class="container">
    <h2 class="text-center mb-4">{{__ "testimonialsTitle"}}</h2>
    <div
      id="testimonialCarousel"
      class="carousel slide"
      data-ride="carousel"
      data-interval="2000"
    >
      <div class="carousel-inner">
        <div class="carousel-item active">
          <div class="testimonial text-center">
            <p class="lead">{{__ "testimonial1"}}</p>
            <h5>- {{__ "user1"}}</h5>
          </div>
        </div>
        <div class="carousel-item">
          <div class="testimonial text-center">
            <p class="lead">{{__ "testimonial2"}}</p>
            <h5>- {{__ "user2"}}</h5>
          </div>
        </div>
        <div class="carousel-item">
          <div class="testimonial text-center">
            <p class="lead">{{__ "testimonial3"}}</p>
            <h5>- {{__ "user3"}}</h5>
          </div>
        </div>
      </div>
      <a
        class="carousel-control-prev"
        href="#testimonialCarousel"
        role="button"
        data-slide="prev"
      >
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">{{__ "previous"}}</span>
      </a>
      <a
        class="carousel-control-next"
        href="#testimonialCarousel"
        role="button"
        data-slide="next"
      >
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">{{__ "next"}}</span>
      </a>
    </div>
  </div>
</section>

<!-- Call to Action Section -->
<section class="cta py-5 my-5 text-center border-top border-dark">
  <div class="container">

    {{#unless user}}
      <h2>{{__ "ctaTitle"}}</h2>
      <p>{{__ "ctaDesc"}}</p>
      <a class="btn btn-light btn-lg" href="/users/register" role="button">{{__
          "signUpNow"
        }}</a>
    {{/unless}}
  </div>
</section>